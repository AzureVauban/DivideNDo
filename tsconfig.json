{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@theme/*": ["app/theme/*"]
    },
    "skipLibCheck": true
  },
  "extends": "expo/tsconfig.base.json",
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/authentication/auth.tsx"
  ],
  "exclude": ["node_modules"]
}
